{
  "name": "@slauth.io/slauth",
  "version": "1.0.6",
  "description": "CLI that scans directories for usage of aws-sdk and generated IAM Policies",
  "main": "dist/index.js",
  "bin": {
    "slauth": "dist/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc --removeComments --declaration",
    "build-watch": "tsc --watch",
    "prepare": "husky install",
    "lint": "eslint ./src --ext ts,tsx",
    "lint-fix": "eslint ./src --ext ts,tsx --fix --cache",
    "format": "prettier ./src --write",
    "format-check": "prettier ./src --check"
  },
  "keywords": [
    "cli",
    "aws",
    "iam",
    "policies",
    "llm",
    "openai"
  ],
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^11.0.1",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "husky": "^8.0.3",
    "prettier": "^3.1.0",
    "semantic-release": "^22.0.8",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "cli-spinners": "^2.9.1",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "langchain": "^0.0.186",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.21.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
